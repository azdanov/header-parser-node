{
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  }
}
